<h1>Sign up</h1>

<%= form_for @user do |f| %>
<%= f.label "First Name" %>
<%= f.text_field :first_name %><br>
<%= f.label "Last Name" %>
<%= f.text_field :last_name %><br><br>
<%= f.label "Username" %>
<%= f.text_field :user_name %><br>
<%= f.label "password" %>
<%= f.password_field :password %><br>
<%= f.submit %>
<h5 class "cancel"><%= link_to "Cancel", quotes_path%></h5>
<% end %>
