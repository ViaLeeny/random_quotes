<h1>Quotes</h1>


<div class = "all">
<% @quotes.each do |quote| %>

<div class = "quotes">
<h2> <%=  quote.content %> </h2>
<p> <%= link_to "-" + quote.source.author.name, author_path(quote.source.author) %></p>
<p> <%= link_to quote.topic.name, topic_path(quote.topic) %></p>
</div>
<% end %>

</div>
