<h1>Quotes</h1>

<% @quotes.each do |quote| %>
  <h2> <%=  quote.content %> </h2>
  <p> <%= link_to "-" + quote.source.author.name, author_path(quote.source.author) %>
      <%= form_tag edit_quote_path(quote), method: "get" do %>
        <%= submit_tag "Edit quote" %>
       <% end %>
     <%= form_tag quote_path(quote), method: "delete" do %>
       <%= submit_tag "Delete quote" %>
      <% end %></p>
<% end %>
